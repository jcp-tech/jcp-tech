{
  "hosting": [
    {
      "target": "prod",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [ {
        "source": "**",
        "destination": "https://jcp-tech.cloud",
        "type": 301
      } ]
    },
    {
      "target": "test",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [ {
        "source": "**",
        "destination": "https://jcp-tech.cloud",
        "type": 301
      } ]
    },
    {
      "target": "old",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "backup",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ]
}
